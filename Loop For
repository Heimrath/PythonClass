for numero in range(0,5):
    print("oi")

print()

for numero in range(1,8):
    print(numero)

print()

for numero in range(1,7):
    if numero > 2:
        print(numero, "é maior do que 2")
    else:
        print(numero, "não é maior do que 2")

print()

quantidade = 0
for letra in "batata":
    if letra == "a":
        quantidade = quantidade + 1

print("Batata possui {} letra(s) 'A'".format(quantidade))
print()

juros = float(input("Digite os juros mensais (em porcentagem %): "))
capital = float(input("Digite o capital: "))
periodo = int(input("Digite o periodo (meses): "))

for i in range(0, periodo):
    capital = capital + (capital * (juros / 100))
    print(capital)
